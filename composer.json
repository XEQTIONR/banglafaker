    "keywords": [
        "tusharkhan",
        "banglafaker",
        "laravel-bangla-faker"
    ],
    "homepage": "https://github.com/tusharkhan/banglafaker",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Tushar Khan",
            "email": "tushar.khan0122@gmail.com",
            "role": "Developer"
        }
