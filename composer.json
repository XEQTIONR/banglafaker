{
    "name": "xeqtionr/banglafaker",
    "description": "A Bangla xeqtionr/Faker Library for Laravel",
    "keywords": [
        "xeqtionr",
        "banglafaker",
        "laravel-bangla-faker"
    ],
    "homepage": "https://github.com/tusharkhan/banglafaker",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ishtehar Hussain",
            "email": "x.e.q.tionrz@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Xeqtionr\\BanglaFaker\\": "src",
            "Xeqtionr\\BanglaFaker\\Lib\\" : "Lib"
        }
    },
    "autoload-dev": {
    },
    "scripts": {
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\Xeqtionr\\BanglaFaker\\BanglaFakerServiceProvider"
            ],
            "aliases": {
                "Banglafaker": "\\Xeqtionr\\BanglaFaker\\BanglaFakerFacade"
            }
        }
    }
}
